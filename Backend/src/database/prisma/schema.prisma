generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model transactions {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  categoria  String
  created_at DateTime @db.Date
  id_        String   @map("id")
  tipo       String
  titulo     String
  valor      Float
}
